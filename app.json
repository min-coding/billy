{
  "expo": {
    "name": "billy",
    "slug": "billy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.billy",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Billy uses your camera to let you take photos of receipts and payment slips for bill verification.",
        "NSPhotoLibraryUsageDescription": "Billy needs access to your photo library so you can upload receipts, payment slips, and profile pictures."
      },
      "icon": {
        "light": "./assets/images/ios-light.png",
        "dark": "./assets/images/ios-light.png",
        "tinted": "./assets/images/ios-light.png"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": false
    },
    "android": {
      "package": "com.anonymous.billy",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android-icon.png",
        "monochromeImage": "./assets/images/android-icon.png",
        "backgroundImage": "./assets/images/android-icon.png"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da9ab0ea-9a14-4e1c-9e5c-58465f70540b"
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/da9ab0ea-9a14-4e1c-9e5c-58465f70540b"
    },
    "owner": "mindot20",
    "jsEngine": "jsc"
  }
}
